      "cStandard": "c17",
      "cppStandard": "c++20",
      "intelliSenseMode": "linux-gcc-x64",
      "compileCommands": "${workspaceFolder}/thrift-kv/cpp/build/compile_commands.json"

    }
  ],
  "version": 4
}
